@model IEnumerable<Tuoterekisteri.Models.Specification>

@{
    ViewBag.Title = T.txt[16, L.nr];
}

<h2>@T.txt[16, L.nr]</h2>

<p>
    @Html.ActionLink(T.txt[17, L.nr], "Create")
</p>
<table class="table">
    <tr>
        <th>
            @T.txt[29, L.nr]
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.loan_spec)
        </td>
        <td>
            @Html.ActionLink(T.txt[11, L.nr], "Edit", new { id=item.spec_id }) |
            @Html.ActionLink(T.txt[18, L.nr], "Details", new { id=item.spec_id }) |
            @Html.ActionLink(T.txt[10, L.nr], "Delete", new { id=item.spec_id })
        </td>
    </tr>
}

</table>
