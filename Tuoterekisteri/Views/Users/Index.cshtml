@model IEnumerable<Tuoterekisteri.Models.User>

@{
    ViewBag.Title = T.txt[16, L.nr];
}

<h2>@T.txt[16, L.nr]</h2>

<p>
    @Html.ActionLink(T.txt[17, L.nr], "Create")
</p>
<table class="table table-hover">
    <tr>
        <th>
            @T.txt[2, L.nr]
        </th>
        <th>
            @T.txt[21, L.nr]
        </th>
        <th>
            @T.txt[28, L.nr]
        </th>
        <th>
            @T.txt[22, L.nr]
        </th>
        <th>
            @T.txt[23, L.nr]
        </th>
        <th>Admin</th>

    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.username)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lastSeen)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.firstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.lastName)
    </td>
    @if  (item.admin == "1")
    { 
        <td>Admin</td>      
    }
    else
    {
        <td>-</td>
    }


    <td>
        @Html.ActionLink(T.txt[11, L.nr], "Edit", new { id = item.user_id }) |
        @Html.ActionLink(T.txt[10, L.nr], "Delete", new { id = item.user_id })
    </td>
</tr>
    }

</table>
